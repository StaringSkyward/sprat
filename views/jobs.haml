%h2= "Jobs"

%table.table.table-bordered.table-hover
  %thead
    %th ID
    %th Created
    %th Host
    %th Spreadsheet
    %th Worksheet
    %th Status
  %tbody
  - @jobs.each do |job|
    %tr
      %td= job.id
      %td= job.created
      %td= job.host
      %td= job.spreadsheet
      %td= job.worksheet
      %td
        %a{title: "#{job.status}", href: "/jobs/#{job.id}"} #{job.status}
